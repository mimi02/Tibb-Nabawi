    <!-- http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#ibuprofen -->

    <owl:Class rdf:about="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#ibuprofen">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#ModernTimeDrugs"/>
        <rdfs:subClassOf>
