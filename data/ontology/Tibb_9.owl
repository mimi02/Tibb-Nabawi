                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#Fever"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
