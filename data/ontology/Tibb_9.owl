    <owl:Class rdf:about="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#gentamicin">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#antibiotics"/>
        <rdfs:subClassOf>
            <owl:Restriction>
